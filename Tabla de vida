137412+133215+131863
Nacimientos<- 416576
mx<- ((Muertes/3)/Pob)
qx<- ((2*mx)/(2+mx))
qx[1] <- (Muertes [1]/3)/(Nacimientos/3)
qx[101] <- 1
px<-(1-qx)
lx <- numeric (101) #nÃºmero de grupos de edad
lx[1] <- 100000 #declaramos el primer valor de lx que es la pob hipotetica de 100k
for(i in 1:100){
  lx[i+1]<- ceiling(lx[i]*(1-qx[i]))}
dx<- qx*lx
Lx<-numeric(101)
Lx[1]<-0.3*lx[1]+0.7*lx[2]
for(i in 2:100){
  Lx[i]<-lx[1+i]+1/2*dx[i]}
Tx<-numeric(101)
for(i in 1:101){
  Tx[i]<- sum(Lx[i:101])
}
ex <- Tx/ lx
TablaVida <- data.frame (Muertes, Pob, mx, qx,px,dx,lx,Lx,Tx,ex)
View(TablaVida)
